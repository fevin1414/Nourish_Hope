{
  "version": 2,
  "builds": [
    { "src": "server/app.js", "use": "@vercel/node" },
    { "src": "client/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server/app.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "client/index.html" }
  ]
}
